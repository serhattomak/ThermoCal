<main id="main" class="main">
    <div class="pagetitle">
        <h1>Hesaplama</h1>
        <nav>
            <ol class="breadcrumb">
                <li class="breadcrumb-item"><a asp-controller="Home" asp-action="Index">Anasayfa</a></li>
                <li class="breadcrumb-item">Hesaplama</li>
                <li class="breadcrumb-item active">Lüle Hesaplama</li>
            </ol>
        </nav>
    </div><!-- End Page Title -->
    <section class="section">
        <div class="row">
            <div class="col-lg-12">
                <div class="card">
                    <div class="card-body">
                        <h5 class="card-title">Lüle Hesaplama</h5>

                        <!-- Floating Labels Form -->
                        <form class="row g-3" id="nozzleCalculationForm">
                            <div class="col-md-6">
                                <div class="form-floating">
                                    <input type="text" class="form-control" id="g" name="G" placeholder="Kütle Akışı (G)">
                                    <label for="g">Kütle Akışı (G)</label>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-floating">
                                    <input type="text" class="form-control" id="a" name="A" placeholder="Alan (A)">
                                    <label for="a">Alan (A)</label>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-floating">
                                    <input type="text" class="form-control" id="density" name="Density" placeholder="Yoğunluk (ρ)">
                                    <label for="density">Yoğunluk (ρ)</label>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-floating">
                                    <input type="text" class="form-control" id="vFirst" name="VFirst" placeholder="İlk Hacim (V1)">
                                    <label for="vFirst">İlk Hacim (V1)</label>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-floating">
                                    <input type="text" class="form-control" id="vSecond" name="VSecond" placeholder="İkinci Hacim (V2)">
                                    <label for="vSecond">İkinci Hacim (V2)</label>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-floating">
                                    <input type="text" class="form-control" id="hFirst" name="HFirst" placeholder="İlk Entalpi (H1)">
                                    <label for="hFirst">İlk Entalpi (H1)</label>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-floating">
                                    <input type="text" class="form-control" id="hSecond" name="HSecond" placeholder="İkinci Entalpi (H2)">
                                    <label for="hSecond">İkinci Entalpi (H2)</label>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-floating">
                                    <input type="text" class="form-control" id="qDot" name="QDot" placeholder="Isı Transfer Oranı (Q̇)">
                                    <label for="qDot">Isı Transfer Oranı (Q̇)</label>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-floating">
                                    <input type="text" class="form-control" id="wDot" name="WDot" placeholder="İş Transfer Oranı (Ẇ)">
                                    <label for="wDot">İş Transfer Oranı (Ẇ)</label>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-floating">
                                    <input type="text" class="form-control" id="mDot" name="MDot" placeholder="Kütlesel Akış Oranı (ṁ)">
                                    <label for="mDot">Kütlesel Akış Oranı (ṁ)</label>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-floating">
                                    <input type="text" class="form-control" id="mDotFirst" name="MDotFirst" placeholder="İlk Kütlesel Akış Oranı (ṁ1)">
                                    <label for="mDotFirst">İlk Kütlesel Akış Oranı (ṁ1)</label>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-floating">
                                    <input type="text" class="form-control" id="mDotSecond" name="MDotSecond" placeholder="İkinci Kütlesel Akış Oranı (ṁ2)">
                                    <label for="mDotSecond">İkinci Kütlesel Akış Oranı (ṁ2)</label>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-floating">
                                    <input type="text" class="form-control" id="zFirst" name="ZFirst" placeholder="İlk Yükseklik (Z1)">
                                    <label for="zFirst">İlk Yükseklik (Z1)</label>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-floating">
                                    <input type="text" class="form-control" id="zSecond" name="ZSecond" placeholder="İkinci Yükseklik (Z2)">
                                    <label for="zSecond">İkinci Yükseklik (Z2)</label>
                                </div>
                            </div>
                            <div class="text-center">
                                <button type="button" class="btn btn-primary" id="calculate">Hesapla</button>
                                <button type="reset" class="btn btn-secondary">Sıfırla</button>
                            </div>
                        </form><!-- End floating Labels Form -->
                    </div>
                </div>
            </div>
        </div>
    </section>
</main>
