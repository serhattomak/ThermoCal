<main id="main" class="main">
    <div class="pagetitle">
        <h1>Hesaplama</h1>
        <nav>
            <ol class="breadcrumb">
                <li class="breadcrumb-item"><a asp-controller="Home" asp-action="Index">Anasayfa</a></li>
                <li class="breadcrumb-item">Hesaplama</li>
                <li class="breadcrumb-item active">Isı Makineleri için Hesaplama</li>
            </ol>
        </nav>
    </div><!-- End Page Title -->
    <section class="section">
        <div class="row">
            <div class="col-lg-12">
                <div class="card">
                    <div class="card-body">
                        <h5 class="card-title">Isı Makineleri için Hesaplama</h5>

                        <!-- Floating Labels Form -->
                        <form class="row g-3" id="heatEngineCalculationForm">
                            <div class="col-md-6">
                                <div class="form-floating">
                                    <input type="text" class="form-control" id="efficiency" name="Efficiency" placeholder="Verimlilik">
                                    <label for="efficiency">Verimlilik</label>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-floating">
                                    <input type="text" class="form-control" id="copHeat" name="COPHeat" placeholder="Isı Pompası COP">
                                    <label for="copHeat">Isı Pompası COP</label>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-floating">
                                    <input type="text" class="form-control" id="copRef" name="COPRef" placeholder="Soğutucu COP">
                                    <label for="copRef">Soğutucu COP</label>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-floating">
                                    <input type="text" class="form-control" id="q" name="Q" placeholder="Isı (Q)">
                                    <label for="q">Isı (Q)</label>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-floating">
                                    <input type="text" class="form-control" id="w" name="W" placeholder="İş (W)">
                                    <label for="w">İş (W)</label>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-floating">
                                    <input type="text" class="form-control" id="qh" name="Qh" placeholder="Yüksek Sıcaklıkta Isı (Qh)">
                                    <label for="qh">Yüksek Sıcaklıkta Isı (Qh)</label>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-floating">
                                    <input type="text" class="form-control" id="ql" name="Ql" placeholder="Düşük Sıcaklıkta Isı (Ql)">
                                    <label for="ql">Düşük Sıcaklıkta Isı (Ql)</label>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-floating">
                                    <input type="text" class="form-control" id="th" name="Th" placeholder="Yüksek Sıcaklık (Th)">
                                    <label for="th">Yüksek Sıcaklık (Th)</label>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-floating">
                                    <input type="text" class="form-control" id="tl" name="Tl" placeholder="Düşük Sıcaklık (Tl)">
                                    <label for="tl">Düşük Sıcaklık (Tl)</label>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-floating">
                                    <input type="text" class="form-control" id="wIn" name="WIn" placeholder="Giriş İş (Win)">
                                    <label for="wIn">Giriş İş (Win)</label>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-floating">
                                    <input type="text" class="form-control" id="wOut" name="WOut" placeholder="Çıkış İş (Wout)">
                                    <label for="wOut">Çıkış İş (Wout)</label>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-floating">
                                    <input type="text" class="form-control" id="wNet" name="WNet" placeholder="Net İş (Wnet)">
                                    <label for="wNet">Net İş (Wnet)</label>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-floating">
                                    <input type="text" class="form-control" id="qIn" name="QIn" placeholder="Giriş Isı (Qin)">
                                    <label for="qIn">Giriş Isı (Qin)</label>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-floating">
                                    <input type="text" class="form-control" id="qOut" name="QOut" placeholder="Çıkış Isı (Qout)">
                                    <label for="qOut">Çıkış Isı (Qout)</label>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-floating">
                                    <input type="text" class="form-control" id="qNet" name="QNet" placeholder="Net Isı (Qnet)">
                                    <label for="qNet">Net Isı (Qnet)</label>
                                </div>
                            </div>
                            <div class="text-center">
                                <button type="button" class="btn btn-primary" id="calculate">Hesapla</button>
                                <button type="reset" class="btn btn-secondary">Sıfırla</button>
                            </div>
                        </form><!-- End floating Labels Form -->
                    </div>
                </div>
            </div>
        </div>
    </section>
</main>
