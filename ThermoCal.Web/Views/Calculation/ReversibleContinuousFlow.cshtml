<main id="main" class="main">
    <div class="pagetitle">
        <h1>Hesaplama</h1>
        <nav>
            <ol class="breadcrumb">
                <li class="breadcrumb-item"><a asp-controller="Home" asp-action="Index">Anasayfa</a></li>
                <li class="breadcrumb-item">Hesaplama</li>
                <li class="breadcrumb-item active">Tersinir Sürekli Akış Hesaplama</li>
            </ol>
        </nav>
    </div><!-- End Page Title -->
    <section class="section">
        <div class="row">
            <div class="col-lg-12">
                <div class="card">
                    <div class="card-body">
                        <h5 class="card-title">Tersinir Sürekli Akış Hesaplama</h5>

                        <!-- Floating Labels Form -->
                        <form class="row g-3" id="reversibleContinuousFlowCalculationForm">
                            <div class="col-md-6">
                                <div class="form-floating">
                                    <input type="text" class="form-control" id="wTr" name="WTr" placeholder="İş Transferi (WTr)">
                                    <label for="wTr">İş Transferi (WTr)</label>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-floating">
                                    <input type="text" class="form-control" id="vSpecific" name="VSpecific" placeholder="Özgül Hacim (v)">
                                    <label for="vSpecific">Özgül Hacim (v)</label>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-floating">
                                    <input type="text" class="form-control" id="p" name="P" placeholder="Basınç (P)">
                                    <label for="p">Basınç (P)</label>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-floating">
                                    <input type="text" class="form-control" id="pFirst" name="PFirst" placeholder="İlk Basınç (P1)">
                                    <label for="pFirst">İlk Basınç (P1)</label>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-floating">
                                    <input type="text" class="form-control" id="pSecond" name="PSecond" placeholder="İkinci Basınç (P2)">
                                    <label for="pSecond">İkinci Basınç (P2)</label>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-floating">
                                    <input type="text" class="form-control" id="velocityFirst" name="VelocityFirst" placeholder="İlk Hız (V1)">
                                    <label for="velocityFirst">İlk Hız (V1)</label>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-floating">
                                    <input type="text" class="form-control" id="velocitySecond" name="VelocitySecond" placeholder="İkinci Hız (V2)">
                                    <label for="velocitySecond">İkinci Hız (V2)</label>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-floating">
                                    <input type="text" class="form-control" id="zFirst" name="ZFirst" placeholder="İlk Yükseklik (Z1)">
                                    <label for="zFirst">İlk Yükseklik (Z1)</label>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-floating">
                                    <input type="text" class="form-control" id="zSecond" name="ZSecond" placeholder="İkinci Yükseklik (Z2)">
                                    <label for="zSecond">İkinci Yükseklik (Z2)</label>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-floating">
                                    <input type="text" class="form-control" id="g" name="G" placeholder="Yerçekimi İvmesi (G)">
                                    <label for="g">Yerçekimi İvmesi (G)</label>
                                </div>
                            </div>
                            <div class="text-center">
                                <button type="button" class="btn btn-primary" id="calculate">Hesapla</button>
                                <button type="reset" class="btn btn-secondary">Sıfırla</button>
                            </div>
                        </form><!-- End floating Labels Form -->
                    </div>
                </div>
            </div>
        </div>
    </section>
</main>
