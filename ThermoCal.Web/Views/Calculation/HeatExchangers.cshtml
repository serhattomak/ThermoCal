<main id="main" class="main">
    <div class="pagetitle">
        <h1>Hesaplama</h1>
        <nav>
            <ol class="breadcrumb">
                <li class="breadcrumb-item"><a asp-controller="Home" asp-action="Index">Anasayfa</a></li>
                <li class="breadcrumb-item">Hesaplama</li>
                <li class="breadcrumb-item active">Isı Değiştiricileri için Hesaplama</li>
            </ol>
        </nav>
    </div><!-- End Page Title -->
    <section class="section">
        <div class="row">
            <div class="col-lg-12">
                <div class="card">
                    <div class="card-body">
                        <h5 class="card-title">Isı Değiştiricileri için Hesaplama</h5>

                        <!-- Floating Labels Form -->
                        <form class="row g-3" id="heatExchangersCalculationForm">
                            <div class="col-md-6">
                                <div class="form-floating">
                                    <input type="text" class="form-control" id="mDotIn" name="MDotIn" placeholder="Giriş Kütlesel Debi (ṁin)">
                                    <label for="mDotIn">Giriş Kütlesel Debi (ṁin)</label>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-floating">
                                    <input type="text" class="form-control" id="mDotOut" name="MDotOut" placeholder="Çıkış Kütlesel Debi (ṁout)">
                                    <label for="mDotOut">Çıkış Kütlesel Debi (ṁout)</label>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-floating">
                                    <input type="text" class="form-control" id="hIn" name="HIn" placeholder="Giriş Enthalpi (Hin)">
                                    <label for="hIn">Giriş Enthalpi (Hin)</label>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-floating">
                                    <input type="text" class="form-control" id="hOut" name="HOut" placeholder="Çıkış Enthalpi (Hout)">
                                    <label for="hOut">Çıkış Enthalpi (Hout)</label>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-floating">
                                    <input type="text" class="form-control" id="vIn" name="VIn" placeholder="Giriş Hacmi (Vin)">
                                    <label for="vIn">Giriş Hacmi (Vin)</label>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-floating">
                                    <input type="text" class="form-control" id="vOut" name="VOut" placeholder="Çıkış Hacmi (Vout)">
                                    <label for="vOut">Çıkış Hacmi (Vout)</label>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-floating">
                                    <input type="text" class="form-control" id="zIn" name="ZIn" placeholder="Giriş Yüksekliği (Zin)">
                                    <label for="zIn">Giriş Yüksekliği (Zin)</label>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-floating">
                                    <input type="text" class="form-control" id="zOut" name="ZOut" placeholder="Çıkış Yüksekliği (Zout)">
                                    <label for="zOut">Çıkış Yüksekliği (Zout)</label>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-floating">
                                    <input type="text" class="form-control" id="g" name="G" placeholder="Kütle Akışı (G)">
                                    <label for="g">Kütle Akışı (G)</label>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-floating">
                                    <input type="text" class="form-control" id="qDot" name="QDot" placeholder="Isı Transfer Oranı (Q̇)">
                                    <label for="qDot">Isı Transfer Oranı (Q̇)</label>
                                </div>
                            </div>
                            <div class="text-center">
                                <button type="button" class="btn btn-primary" id="calculate">Hesapla</button>
                                <button type="reset" class="btn btn-secondary">Sıfırla</button>
                            </div>
                        </form><!-- End floating Labels Form -->
                    </div>
                </div>
            </div>
        </div>
    </section>
</main>
