<main id="main" class="main">
    <div class="pagetitle">
        <h1>Hesaplama</h1>
        <nav>
            <ol class="breadcrumb">
                <li class="breadcrumb-item"><a asp-controller="Home" asp-action="Index">Anasayfa</a></li>
                <li class="breadcrumb-item">Hesaplama</li>
                <li class="breadcrumb-item active">Pompa Hesaplama</li>
            </ol>
        </nav>
    </div><!-- End Page Title -->
    <section class="section">
        <div class="row">
            <div class="col-lg-12">
                <div class="card">
                    <div class="card-body">
                        <h5 class="card-title">Pompa Hesaplama</h5>

                        <!-- Floating Labels Form -->
                        <form class="row g-3" id="pumpCalculationForm">
                            <div class="col-md-6">
                                <div class="form-floating">
                                    <input type="text" class="form-control" id="wmDot" name="WMDot" placeholder="Kütlesel Akış İş Oranı (Ẇ/ṁ)">
                                    <label for="wmDot">Kütlesel Akış İş Oranı (Ẇ/ṁ)</label>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-floating">
                                    <input type="text" class="form-control" id="mDot" name="MDot" placeholder="Kütlesel Akış Oranı (ṁ)">
                                    <label for="mDot">Kütlesel Akış Oranı (ṁ)</label>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-floating">
                                    <input type="text" class="form-control" id="g" name="G" placeholder="Yerçekimi İvmesi (G)">
                                    <label for="g">Yerçekimi İvmesi (G)</label>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-floating">
                                    <input type="text" class="form-control" id="hFirst" name="HFirst" placeholder="İlk Entalpi (H1)">
                                    <label for="hFirst">İlk Entalpi (H1)</label>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-floating">
                                    <input type="text" class="form-control" id="hSecond" name="HSecond" placeholder="İkinci Entalpi (H2)">
                                    <label for="hSecond">İkinci Entalpi (H2)</label>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-floating">
                                    <input type="text" class="form-control" id="zFirst" name="ZFirst" placeholder="İlk Yükseklik (Z1)">
                                    <label for="zFirst">İlk Yükseklik (Z1)</label>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-floating">
                                    <input type="text" class="form-control" id="zSecond" name="ZSecond" placeholder="İkinci Yükseklik (Z2)">
                                    <label for="zSecond">İkinci Yükseklik (Z2)</label>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-floating">
                                    <input type="text" class="form-control" id="pFirst" name="PFirst" placeholder="İlk Basınç (P1)">
                                    <label for="pFirst">İlk Basınç (P1)</label>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-floating">
                                    <input type="text" class="form-control" id="pSecond" name="PSecond" placeholder="İkinci Basınç (P2)">
                                    <label for="pSecond">İkinci Basınç (P2)</label>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-floating">
                                    <input type="text" class="form-control" id="tFirst" name="TFirst" placeholder="İlk Sıcaklık (T1)">
                                    <label for="tFirst">İlk Sıcaklık (T1)</label>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-floating">
                                    <input type="text" class="form-control" id="tSecond" name="TSecond" placeholder="İkinci Sıcaklık (T2)">
                                    <label for="tSecond">İkinci Sıcaklık (T2)</label>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-floating">
                                    <input type="text" class="form-control" id="uFirst" name="UFirst" placeholder="İlk İç Enerji (U1)">
                                    <label for="uFirst">İlk İç Enerji (U1)</label>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-floating">
                                    <input type="text" class="form-control" id="uSecond" name="USecond" placeholder="İkinci İç Enerji (U2)">
                                    <label for="uSecond">İkinci İç Enerji (U2)</label>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-floating">
                                    <input type="text" class="form-control" id="c" name="C" placeholder="Isı Kapasitesi (C)">
                                    <label for="c">Isı Kapasitesi (C)</label>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-floating">
                                    <input type="text" class="form-control" id="vSpecific" name="VSpecific" placeholder="Özgül Hacim (v)">
                                    <label for="vSpecific">Özgül Hacim (v)</label>
                                </div>
                            </div>
                            <div class="text-center">
                                <button type="button" class="btn btn-primary" id="calculate">Hesapla</button>
                                <button type="reset" class="btn btn-secondary">Sıfırla</button>
                            </div>
                        </form><!-- End floating Labels Form -->
                    </div>
                </div>
            </div>
        </div>
    </section>
</main>
