<main id="main" class="main">
    <div class="pagetitle">
        <h1>Hesaplama</h1>
        <nav>
            <ol class="breadcrumb">
                <li class="breadcrumb-item"><a asp-controller="Home" asp-action="Index">Anasayfa</a></li>
                <li class="breadcrumb-item">Hesaplama</li>
                <li class="breadcrumb-item active">Kompresör Hesaplama</li>
            </ol>
        </nav>
    </div><!-- End Page Title -->
    <section class="section">
        <div class="row">
            <div class="col-lg-12">
                <div class="card">
                    <div class="card-body">
                        <h5 class="card-title">Kompresör Hesaplama</h5>

                        <!-- Floating Labels Form -->
                        <form class="row g-3">
                            <div class="col-md-6">
                                <div class="form-floating">
                                    <input type="text" class="form-control" id="wDotM" placeholder="Mil İşi (Wmil)">
                                    <label for="wDotM">Mil İşi (Wmil)</label>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-floating">
                                    <input type="text" class="form-control" id="mDot" placeholder="Kütlesel Debi (ṁ)">
                                    <label for="mDot">Kütlesel Debi (ṁ)</label>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-floating">
                                    <input type="text" class="form-control" id="mDotFirst" placeholder="Kütlesel Debi (ṁ1)">
                                    <label for="mDotFirst">Kütlesel Debi (ṁ1)</label>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-floating">
                                    <input type="text" class="form-control" id="mDotSecond" placeholder="Kütlesel Debi (ṁ2)">
                                    <label for="mDotSecond">Kütlesel Debi (ṁ2)</label>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-floating">
                                    <input type="text" class="form-control" id="densityFirst" placeholder="Yoğunluk (ρ1)">
                                    <label for="densityFirst">Yoğunluk (ρ1)</label>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-floating">
                                    <input type="text" class="form-control" id="densitySecond" placeholder="Yoğunluk (ρ2)">
                                    <label for="densitySecond">Yoğunluk (ρ2)</label>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-floating">
                                    <input type="text" class="form-control" id="areaFirst" placeholder="Alan (A1)">
                                    <label for="areaFirst">Alan (A1)</label>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-floating">
                                    <input type="text" class="form-control" id="areaSecond" placeholder="Alan (A2)">
                                    <label for="areaSecond">Alan (A2)</label>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-floating">
                                    <input type="text" class="form-control" id="vFirst" placeholder="Hacim (V1)">
                                    <label for="vFirst">Hacim (V1)</label>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-floating">
                                    <input type="text" class="form-control" id="vSecond" placeholder="Hacim (V2)">
                                    <label for="vSecond">Hacim (V2)</label>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-floating">
                                    <input type="text" class="form-control" id="hFirst" placeholder="Entalpi (H1)">
                                    <label for="hFirst">Entalpi (H1)</label>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-floating">
                                    <input type="text" class="form-control" id="hSecond" placeholder="Entalpi (H2)">
                                    <label for="hSecond">Entalpi (H2)</label>
                                </div>
                            </div>
                            <div class="text-center">
                                <button type="button" class="btn btn-primary" id="calculate">Hesapla</button>
                                <button type="reset" class="btn btn-secondary">Sıfırla</button>
                            </div>
                        </form><!-- End floating Labels Form -->
                    </div>
                </div>
            </div>
        </div>
    </section>
</main>